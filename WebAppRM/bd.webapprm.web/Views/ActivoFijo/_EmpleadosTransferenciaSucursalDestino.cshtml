@model CambioUbicacionSucursalViewModel
<div class="row">
    <section class="col col-4">
        <label asp-for="IdEmpleadoRecibe"></label>
    </section>

    <section class="col col-8">
        <label class="input">
            <select asp-for="IdEmpleadoRecibe" class="form-control" asp-items="ViewBag.EmpleadoRecibe"></select>
            <span asp-validation-for="IdEmpleadoRecibe" class="text-danger"></span>
        </label>
    </section>
</div>

<div class="row">
    <section class="col col-4">
        <label asp-for="IdEmpleadoResponsableRecibo"></label>
    </section>

    <section class="col col-8">
        <label class="input">
            <select asp-for="IdEmpleadoResponsableRecibo" class="form-control" asp-items="ViewBag.EmpleadoResponsableRecibo"></select>
            <span asp-validation-for="IdEmpleadoResponsableRecibo" class="text-danger"></span>
        </label>
    </section>
</div>