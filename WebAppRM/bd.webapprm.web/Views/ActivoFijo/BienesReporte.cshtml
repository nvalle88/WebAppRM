@model List<bd.webapprm.entidades.RecepcionActivoFijoDetalle>
<script src="~/lib/JsBarcode-master/dist/JsBarcode.all.min.js"></script>
<div id="content">
    <section id="widget-grid" class="">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget jarviswidget-color-darken" id="wid-id-0" data-widget-editbutton="false">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Reportes</h2>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox"></div>
                        <div class="widget-body no-padding">
                            <div class="padding-10">
                                <br>
                                <div class="pull-left">
                                    <h1 style="font-size:260% !important;"><i class="fa fa-file-text-o"></i> Bienes por &aacute;rea usuaria y clasificado por funcionario</h1>
                                </div>
                                <div class="pull-right">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div>
                                                <div class="font-md">
                                                    <strong>Fecha:&nbsp;&nbsp;&nbsp;&nbsp;</strong>
                                                    <span class="pull-right"> <i class="fa fa-calendar"></i>&nbsp;&nbsp;@DateTime.Now.ToString("dd/MM/yyyy hh:mm tt") </span>
                                                </div>
                                            </div>
                                            <br>
                                            <div class="well well-sm  bg-color-darken txt-color-white no-border">
                                                <div class="fa-lg">
                                                    Total de Activos Fijos:
                                                    <span class="pull-right"> @Model.Count </span>
                                                </div>
                                            </div>
                                            <br>
                                            <br>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>

                                <table id="tbReporte" class="table table-striped table-bordered table-hover" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th>
                                                Pa&iacute;s
                                            </th>
                                            <th>
                                                Provincia
                                            </th>
                                            <th>
                                                Ciudad
                                            </th>
                                            <th>
                                                Sucursal
                                            </th>
                                            <th>
                                                Funcionario
                                            </th>
                                            <th>
                                                Tipo
                                            </th>
                                            <th>
                                                Clase
                                            </th>
                                            <th>
                                                Subclase
                                            </th>
                                            <th>
                                                Motivo de Recepci&oacute;n
                                            </th>
                                            <th>
                                                Fecha de Registro
                                            </th>
                                            <th>
                                                Proveedor
                                            </th>
                                            <th>
                                                Fondo
                                            </th>
                                            <th>
                                                Orden de compra
                                            </th>
                                            <th>
                                                Cantidad
                                            </th>
                                            <th>
                                                Marca
                                            </th>
                                            <th>
                                                Modelo
                                            </th>
                                            <th>
                                                Unidad de medida
                                            </th>
                                            <th>
                                                Activo Fijo
                                            </th>
                                            <th>
                                                Serie
                                            </th>
                                            <th>
                                                Ubicaci&oacute;n
                                            </th>
                                            <th>
                                                Valor de compra
                                            </th>
                                            @*<th>
                                                C&oacute;digo &Uacute;nico
                                            </th>
                                            <th>
                                                C&oacute;digo de barras
                                            </th>*@
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.LibroActivoFijo.Sucursal.Ciudad.Provincia.Pais.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.LibroActivoFijo.Sucursal.Ciudad.Provincia.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.LibroActivoFijo.Sucursal.Ciudad.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.LibroActivoFijo.Sucursal.Nombre)
                                                </td>

                                                <td>
                                                    @{ await ViewContext.Writer.WriteAsync(String.Format("{0} {1}", item.RecepcionActivoFijo.Empleado.Persona.Nombres, item.RecepcionActivoFijo.Empleado.Persona.Apellidos)); }
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.SubClaseActivoFijo.ClaseActivoFijo.TipoActivoFijo.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.SubClaseActivoFijo.ClaseActivoFijo.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.SubClaseActivoFijo.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.MotivoRecepcion.Descripcion)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.FechaRecepcion)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.Proveedor.RazonSocial)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.Fondo)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.OrdenCompra)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.RecepcionActivoFijo.Cantidad)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.Modelo.Marca.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.Modelo.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.UnidadMedida.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.Nombre)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.Serie)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.Ubicacion)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.ValorCompra)
                                                </td>

                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.ActivoFijo.CodigoActivoFijo.Codigosecuencial)
                                                </td>

                                                <td>
                                                    <img id="barcode_@item.ActivoFijo.CodigoActivoFijo.IdCodigoActivoFijo" />
                                                    @{
                                                        <script type="text/javascript">
                                                            setTimeout(function () {
                                                                var idElemento = "barcode_" + "@item.ActivoFijo.CodigoActivoFijo.IdCodigoActivoFijo";
                                                                var valor = "@item.ActivoFijo.CodigoActivoFijo.CodigoBarras";
                                                                Asignar_Codigo_Barras(idElemento, valor);
                                                            });
                                                        </script>
                                                    }
                                                </td>*@
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                                <div class="invoice-footer">
                                    <div class="row">
                                        <div class="col-sm-7"></div>
                                        <div class="col-sm-5">
                                            <div class="invoice-sum-total pull-right">
                                                <h3><strong>Total de Activos Fijos: <span class="text-success">@Model.Count</span></strong></h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>
@section Scripts{
    <script src="~/lib/TableTools-master/src/dataTables.tableTools.js"></script>
    <script src="~/js/activoFijo/reporteBienes.min.js"></script>
}